$(function(){function hideMobMenu(event){!isMobMenuOpen||menu===event.target||menu[0].contains(event.target)||mobMenu===event.target||mobMenu[0].contains(event.target)||(menu.css("display","none").html(""),mobMenu.removeClass("mobmenu__show"),menuTrigger.removeClass("active"),isMenuOpen=!1,isMobMenuOpen=!1,win.off("click.menu"))}function hideMenu(event){!isMenuOpen||menu===event.target||menu[0].contains(event.target)||mobMenu===event.target||mobMenu[0].contains(event.target)||(menu.css("display","none").html(""),mobMenu.removeClass("mobmenu__show"),menuTrigger.removeClass("active"),isMenuOpen=!1,isMobMenuOpen=!1,win.off("click.menu"))}var menuTrigger=$(".js-menu-trigger"),isMenuOpen=!1,menu=$(".js-menu"),menuContent=$(".js-menu-content"),isMobMenuOpen=!1,mobMenuTrigger=$(".js-mobmenu-trigger"),mobMenuMinTrigger=$(".js-menumin-trigger"),mobMenu=$(".js-mobmenu"),slider=$(".js-slider"),win=($("#video"),$(window)),initSlider=function(){slider.slick({dots:!0,infinite:!0,speed:300,autoplaySpeed:15e3,slidesToShow:1,autoplay:!0,nextArrow:'<span class="b-slider-button b-slider-button__right"><i class="b-icon b-icon__right"></i></span>',prevArrow:'<span class="b-slider-button b-slider-button__left"><i class="b-icon b-icon__left"></i></span>'})};if(initSlider(),menuTrigger.on("click",function(){var contentCopy=menuContent.clone();isMenuOpen||(menu.html(contentCopy).css("display","block"),menuTrigger.addClass("active"),isMenuOpen=!0,$(".js-menuback-trigger").on("click",function(){mobMenu.addClass("mobmenu__show"),isMobMenuOpen=!0,menu.css("display","none"),isMenuOpen=!1,menuTrigger.removeClass("active"),win.on("click.mobmenu",hideMobMenu)}),win.on("click.menu",hideMenu),event.stopPropagation())}),mobMenuTrigger.on("click",function(){isMobMenuOpen||(mobMenu.addClass("mobmenu__show"),isMobMenuOpen=!0,win.on("click.mobmenu",hideMobMenu),event.stopPropagation())}),mobMenuMinTrigger.on("click",function(){mobMenu.removeClass("mobmenu__show"),isMobMenuOpen=!1,win.off("click.mobmenu")}),/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)){var ww=$(window).width()<window.screen.width?$(window).width():window.screen.width,mw=320,ratio=ww/mw,viewport=$("meta[name=viewport]");ww<mw&&viewport.attr("content","initial-scale="+ratio)}$('a[href^="#anchor_"]').click(function(){var destination,elementClick=$(this).attr("href");return $(elementClick).length>0&&(destination=$(elementClick).offset().top+8,$("html, body").animate({scrollTop:destination},400)),!1})}),$(function(){var $casebookLinks=$("a").filter(function(index,item){return!!$(item).attr("href")&&"/"==$(item).attr("href").substring(0,1)});$.each($casebookLinks,function(index,item){var $item=$(item),itemHref=$item.attr("href")||"",locationSearch=location.search||"";/mailto:/.test(itemHref)||/tel:/.test(itemHref)||/popup:/.test(itemHref)||(/\?/.test(itemHref)&&(locationSearch=locationSearch.replace(/\?/,"&")),$item.attr("href",itemHref+locationSearch))})}),$(function(){function createLinksTimeout(){var newRegLinks=$("a").filter(function(index,item){return/Account\/Register/.test($(item).attr("href"))});newRegLinks.length>0?createPopupLink(newRegLinks):setTimeout(createLinksTimeout,1e3)}function createPopupLink(link){link.removeAttr("href"),link.click(function(event){event.preventDefault(),regPopup.addClass("b-block__show"),popupSubstrate.addClass("b-block__show")})}var regLinks=$("a").filter(function(index,item){return/Account\/Register/.test($(item).attr("href"))}),regPopup=$(".js-reg_popup"),popupSubstrate=$(".js-substrate"),regPopupCompanyBtn=regPopup.find(".js-choise_btn__company"),regPopupPersonBtn=regPopup.find(".js-choise_btn__person"),regPopupHide=$(".js-reg_popup__hide"),searchString=location.search;companySearch=searchString,personSearch=searchString,doc=$(document),utmCompany&&utmPerson&&(companySearch=companySearch.replace(/utm_campaign[^&]+/,"utm_campaign="+utmCompany.utm_campaign),companySearch=companySearch.replace(/utm_medium[^&]+/,"utm_medium="+utmCompany.utm_medium),companySearch=companySearch.replace(/utm_source[^&]+/,"utm_source="+utmCompany.utm_source),personSearch=personSearch.replace(/utm_campaign[^&]+/,"utm_campaign="+utmPerson.utm_campaign),personSearch=personSearch.replace(/utm_medium[^&]+/,"utm_medium="+utmPerson.utm_medium),personSearch=personSearch.replace(/utm_source[^&]+/,"utm_source="+utmPerson.utm_source)),regPopupCompanyBtn.data("href","/Account/Register"+companySearch),regPopupPersonBtn.data("href","/Account/Register"+personSearch),createPopupLink(regLinks),regPopupCompanyBtn.click(function(){dataLayer.push({event:"select_company"}),location.assign($(this).data("href"))}),regPopupCompanyBtn.on("mouseup",function(event){2==event.which&&(dataLayer.push({event:"select_company"}),window.open($(this).data("href"),"_blank"))}),regPopupCompanyBtn.on("mousedown",function(event){2==event.which&&event.preventDefault()}),regPopupPersonBtn.click(function(){dataLayer.push({event:"select_fl"}),location.assign($(this).data("href"))}),regPopupPersonBtn.on("mouseup",function(event){2==event.which&&(dataLayer.push({event:"select_fl"}),window.open($(this).data("href"),"_blank"))}),regPopupPersonBtn.on("mousedown",function(event){2==event.which&&event.preventDefault()}),regPopupHide.click(function(){regPopup.removeClass("b-block__show"),popupSubstrate.removeClass("b-block__show")}),popupSubstrate.click(function(){var activePopups=$(".b-block__show");activePopups.removeClass("b-block__show")}),doc.on("wheel",function(event){popupSubstrate.hasClass("b-block__show")&&event.preventDefault()}),setTimeout(function(){createLinksTimeout()},1e3)});